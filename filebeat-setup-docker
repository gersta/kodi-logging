udo docker run docker.elastic.co/beats/filebeat:7.16.3 setup -E setup.kibana.host=192.168.178.47:5601 -E output.elasticsearch.hosts=["192.168.178.47:9200"]


docker run docker.elastic.co/beats/filebeat:7.16.3 setup --index-management -E output.logstash.enabled=false -E 'output.elasticsearch.hosts=["192.168.178.47:9200"]' -v ./filebeat.docker.yml:/usr/share/filebeat/filebeat.yml -v ./filebeat.template.json:/usr/share/filebeat/filebeat.template.json log-files:/var/logs/kodi:ro -E output.elasticsearch.index="kodi-%{+yyyy.MM.dd}" -E setup.template.name="kodi" -E setup.template.pattern="kodi-*" -E setup.ilm.overwrite=true
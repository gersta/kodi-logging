{
    "index_patterns": ["kodi-*"],
    "priority": 100,
    "template": {
        "mappings": {
            "properties": {
                "@timestamp": {
                    "type": "date",
                    "format": "yyyy-MM-dd HH:mm:ss.SSS"
                },
                "thread-id": {
                    "type": "text"
                },
                "loglevel": {
                    "type": "keyword"
                },
                "message": {
                    "type": "text"
                }
            }
        }
    }
}